<div id="answer-18108000" class="answer" data-answerid="18108000"  itemscope itemtype="http://schema.org/Answer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
        <input type="hidden" name="_id_" value="18108000">
        <a class="vote-up-off" title="This answer is useful">up vote</a>
        <span itemprop="upvoteCount" class="vote-count-post ">19</span>
        <a class="vote-down-off" title="This answer is not useful">down vote</a>




</div>

